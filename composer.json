{
    "name": "esemen/multiclient",
    "description": "Multi-client approach in same instance for Laravel 6+",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ozhan Esemen",
            "email": "dev@esemen.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.2",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Esemen\\MultiClient\\": [
                "src",
                "src/models",
                "src/scopes",
                "src/traits"
            ]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esemen\\MultiClient\\ClientServiceProvider"
            ]
        }
    }
}
